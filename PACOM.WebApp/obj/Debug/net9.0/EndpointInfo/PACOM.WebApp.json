{
  "openapi": "3.0.4",
  "info": {
    "title": "PACOM.WebApp",
    "version": "1.0"
  },
  "paths": {
    "/API/ManageOrganization": {
      "post": {
        "tags": [
          "API"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Organization"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Organization"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Organization"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
