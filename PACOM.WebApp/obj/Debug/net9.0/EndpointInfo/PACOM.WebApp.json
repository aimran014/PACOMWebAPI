      "get": {
        "tags": [
          "API"
        ],
        "parameters": [
          {
            "name": "OrganizationCode",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
